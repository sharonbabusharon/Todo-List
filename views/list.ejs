
<%- include("header") -%>

<div class="box" id="heading">
	<h1><%= listTitle %></h1> 
</div>

<div class="box">
	<% for (let i = 0; i <newOption.length; i++) {%>
	<div class="item">
		<input type="checkbox">
		 <p><%= newOption[i] %></p > 
		</div>
	<%}%>

	<form action="/" method="post" class="item">
		<input type="text" name="newItem" placeholder="New Item" autocomplete="off" >
		<button type="submit" value=<%= listTitle %> name="list">+</button>
	</form>

</div>

<%- include("footer") -%>
		
		
